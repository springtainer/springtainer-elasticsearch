{
	"groups": [],
	"properties": [
		{
			"name": "embedded.container.elasticsearch.enabled",
			"type": "java.lang.Boolean",
			"description": "Enables/Disables container startup.",
			"defaultValue": "true"
		},
		{
			"name": "embedded.container.elasticsearch.startup-timeout",
			"description": "Max time to wait for a successfully started container",
			"type": "java.lang.Integer",
			"defaultValue": "30"
		},
		{
			"name": "embedded.container.elasticsearch.docker-image",
			"type": "java.lang.String",
			"description": "Docker-image",
			"defaultValue": "docker.elastic.co/elasticsearch/elasticsearch:6.8.10"
		},
		{
			"name": "embedded.container.elasticsearch.http-port",
			"description": "Http-Port",
			"type": "java.lang.Integer",
			"defaultValue": "5672"
		},
		{
			"name": "embedded.container.elasticsearch.transport-port",
			"description": "Transport-Port",
			"type": "java.lang.Integer",
			"defaultValue": "15672"
		}
	]
}
